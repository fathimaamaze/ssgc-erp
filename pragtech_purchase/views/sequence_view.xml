        <!-- Sequences for quotation.comparison -->
        <record id="seq_quotation_comparison" model="ir.sequence">
            <field name="name">quotation.comparison.sequence</field>
            <field name="code">quotation.comparison</field>
            <field name="prefix">QC</field>
